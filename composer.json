{
    "name": "bluzphp/container",
    "description": "Container package",
    "type": "library",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "php-coveralls/php-coveralls": "~2.5",
        "squizlabs/php_codesniffer": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "Bluz\\": "src/"
        }
    },
    "autoload-dev": {
